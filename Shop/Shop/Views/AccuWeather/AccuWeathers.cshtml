@model Shop.Models.AccuWeather.AccuWeatherViewModel

<div class="Container">

    <h2> Forecast for the selected city </h2>
    <h2>@Html.DisplayFor(model=>model.City)</h2>
    <hr />
    <dl class="row">

        <div class="form-group row">
            <label asp-for="AccuCitysKey" class="col-sm-2 col-form-label">
            </label>
            @Html.DisplayFor(model=>model.AccuCitysKey)
        </div>

        <div class="form-group row">
            <label asp-for="DailyForecastDate" class="col-sm-2 col-form-label">
            </label>
            @Html.DisplayFor(model=>model.DailyForecastDate)
        </div>

        <div class="form-group row">
            <label asp-for="DailyForecastDaysHasPrecipitation" class="col-sm-2 col-form-label">
            </label>
            @Html.DisplayFor(model=>model.DailyForecastDaysHasPrecipitation)
        </div>

        <div class="form-group row">
            <label asp-for="DailyForecastEpochDate" class="col-sm-2 col-form-label">
            </label>
            @Html.DisplayFor(model=>model.DailyForecastEpochDate)
        </div>
        <div class="form-group row">
            <label asp-for="HeadlinesText" class="col-sm-2 col-form-label">
            </label>
            @Html.DisplayFor(model=>model.HeadlinesText)
        </div>


    </dl>

</div>